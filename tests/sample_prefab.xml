<?xml version="1.0" encoding="UTF-8"?>
<!-- this is an aeroplane prefab. you only need to pass a mesh source -->
<go:gameobject name="aeroplane"
	xmlns:go="file://gameobject.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="file://gameobject.xsd gameobject.xsd">
	
    <gameobject name="collision_body">
        <box_collider>
            <center>0.0 -1.2 0.45</center>
            <size>1.0 13.0 5.3</size>
        </box_collider>
    </gameobject>
    <gameobject name="collision_wings">
        <box_collider>
            <center>0.0 -2.2 0.0</center>
            <scale>8.0 1.7 0.4</scale>
        </box_collider>
    </gameobject>
</go:gameobject>